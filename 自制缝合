function AdvancedUI:CreateScriptCard(scriptData, index)
    local card = Instance.new("Frame")
    card.Size = UDim2.new(1, -20, 0, 100)
    card.Position = UDim2.new(0, 10, 0, (index-1) * 115)
    card.BackgroundColor3 = Color3.fromRGB(35, 40, 70)
    card.BackgroundTransparency = 0.1
    card.BorderSizePixel = 0
    card.Parent = self.scriptListContainer
    
    local cardCorner = Instance.new("UICorner")
    cardCorner.CornerRadius = UDim.new(0, 15)
    cardCorner.Parent = card
    
    -- å¡ç‰‡è¾¹æ¡†
    local cardStroke = Instance.new("UIStroke")
    cardStroke.Color = Color3.fromRGB(80, 120, 200)
    cardStroke.Thickness = 2
    cardStroke.Transparency = 0.3
    cardStroke.Parent = card
    
    -- å›¾æ ‡
    local icon = Instance.new("TextLabel")
    icon.Size = UDim2.new(0, 60, 0, 60)
    icon.Position = UDim2.new(0, 15, 0.5, -30)
    icon.BackgroundTransparency = 1
    icon.Text = scriptData.Icon
    icon.TextColor3 = Color3.fromRGB(255, 255, 255)
    icon.TextSize = 40
    icon.Font = Enum.Font.GothamBlack
    icon.Parent = card
    
    -- è„šæœ¬åç§°
    local name = Instance.new("TextLabel")
    name.Size = UDim2.new(0, 300, 0, 30)
    name.Position = UDim2.new(0, 90, 0, 15)
    name.BackgroundTransparency = 1
    name.Text = scriptData.Name
    name.TextColor3 = Color3.fromRGB(255, 255, 255)
    name.TextSize = 20
    name.Font = Enum.Font.GothamBold
    name.TextXAlignment = Enum.TextXAlignment.Left
    name.Parent = card
    
    -- æè¿°
    local desc = Instance.new("TextLabel")
    desc.Size = UDim2.new(0, 300, 0, 20)
    desc.Position = UDim2.new(0, 90, 0, 40)
    desc.BackgroundTransparency = 1
    desc.Text = scriptData.Description
    desc.TextColor3 = Color3.fromRGB(200, 200, 220)
    desc.TextSize = 14
    desc.Font = Enum.Font.Gotham
    desc.TextXAlignment = Enum.TextXAlignment.Left
    desc.Parent = card
    
    -- ä¿¡æ¯æ ‡ç­¾
    local info = Instance.new("TextLabel")
    info.Size = UDim2.new(0, 200, 0, 20)
    info.Position = UDim2.new(0, 90, 0, 60)
    info.BackgroundTransparency = 1
    info.Text = scriptData.Author .. " â€¢ " .. scriptData.Version .. " â€¢ " .. scriptData.Category
    info.TextColor3 = Color3.fromRGB(150, 150, 180)
    info.TextSize = 12
    info.Font = Enum.Font.Gotham
    info.TextXAlignment = Enum.TextXAlignment.Left
    info.Parent = card
    
    -- æ‰§è¡ŒæŒ‰é’®
    local button = Instance.new("TextButton")
    button.Size = UDim2.new(0, 120, 0, 40)
    button.Position = UDim2.new(1, -140, 0.5, -20)
    button.BackgroundColor3 = Color3.fromRGB(70, 130, 200)
    button.Text = "ğŸš€ æ‰§è¡Œè„šæœ¬"
    button.TextColor3 = Color3.fromRGB(255, 255, 255)
    button.TextSize = 14
    button.Font = Enum.Font.GothamBold
    button.Parent = card
    
    local buttonCorner = Instance.new("UICorner")
    buttonCorner.CornerRadius = UDim.new(0, 10)
    buttonCorner.Parent = button
    
    local buttonStroke = Instance.new("UIStroke")
    buttonStroke.Color = Color3.fromRGB(150, 200, 255)
    buttonStroke.Thickness = 2
    buttonStroke.Parent = button
    
    -- æŒ‰é’®äº¤äº’æ•ˆæœ
    button.MouseEnter:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(90, 160, 240),
            Size = UDim2.new(0, 130, 0, 45)
        }):Play()
    end)
    
    button.MouseLeave:Connect(function()
        TweenService:Create(button, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(70, 130, 200),
            Size = UDim2.new(0, 120, 0, 40)
        }):Play()
    end)
    
    button.MouseButton1Click:Connect(function()
        self:ExecuteScriptWithEffects(scriptData, button)
    end)
    
    -- å¡ç‰‡æ‚¬åœæ•ˆæœ
    card.MouseEnter:Connect(function()
        TweenService:Create(card, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(45, 50, 90),
            BackgroundTransparency = 0.05
        }):Play()
        TweenService:Create(cardStroke, TweenInfo.new(0.3), {
            Color = Color3.fromRGB(100, 150, 255),
            Thickness = 3
        }):Play()
    end)
    
    card.MouseLeave:Connect(function()
        TweenService:Create(card, TweenInfo.new(0.3), {
            BackgroundColor3 = Color3.fromRGB(35, 40, 70),
            BackgroundTransparency = 0.1
        }):Play()
        TweenService:Create(cardStroke, TweenInfo.new(0.3), {
            Color = Color3.fromRGB(80, 120, 200),
            Thickness = 2
        }):Play()
    end)
end

function AdvancedUI:ExecuteScriptWithEffects(scriptData, button)
    -- åˆ›å»ºæ‰§è¡Œæ•ˆæœ
    local originalText = button.Text
    button.Text = "â³ æ‰§è¡Œä¸­..."
    button.AutoButtonColor = false
    
    TweenService:Create(button, TweenInfo.new(0.5), {
        BackgroundColor3 = Color3.fromRGB(255, 200, 50)
    }):Play()
    
    -- åˆ›å»ºåŠ è½½åŠ¨ç”»
    local loadingCircle = Instance.new("Frame")
    loadingCircle.Size = UDim2.new(0, 30, 0, 30)
    loadingCircle.Position = UDim2.new(0.5, -15, 0.5, -15)
    loadingCircle.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
    loadingCircle.BackgroundTransparency = 0.8
    loadingCircle.BorderSizePixel = 0
    loadingCircle.Parent = button
    
    local circleCorner = Instance.new("UICorner")
    circleCorner.CornerRadius = UDim.new(1, 0)
    circleCorner.Parent = loadingCircle
    
    -- æ—‹è½¬åŠ¨ç”»
    local rotation = 0
    local connection
    connection = RunService.Heartbeat:Connect(function(delta)
        rotation = (rotation + delta * 360) % 360
        loadingCircle.Rotation = rotation
    end)
    
    -- å»¶è¿Ÿæ‰§è¡Œä»¥æ˜¾ç¤ºåŠ¨ç”»
    wait(1.5)
    
    -- æ‰§è¡Œè„šæœ¬
    local success, message = pcall(function()
        scriptData.Script()
    end)
    
    -- æ¸…ç†åŠ è½½åŠ¨ç”»
    connection:Disconnect()
    loadingCircle:Destroy()
    
    -- æ˜¾ç¤ºç»“æœ
    if success then
        button.Text = "âœ… æ‰§è¡ŒæˆåŠŸ!"
        TweenService:Create(button, TweenInfo.new(0.5), {
            BackgroundColor3 = Color3.fromRGB(50, 200, 100)
        }):Play()
    else
        button.Text = "âŒ æ‰§è¡Œå¤±è´¥"
        TweenService:Create(button, TweenInfo.new(0.5), {
            BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        }):Play()
    end
    
    -- æ¢å¤æŒ‰é’®çŠ¶æ€
    wait(2)
    button.Text = originalText
    button.AutoButtonColor = true
    TweenService:Create(button, TweenInfo.new(0.5), {
        BackgroundColor3 = Color3.fromRGB(70, 130, 200)
    }):Play()
end

function AdvancedUI:CreateAnimations()
    -- åˆ›å»ºå…¥åœºåŠ¨ç”»
    self.mainContainer.Size = UDim2.new(0, 0, 0, 0)
    self.mainContainer.Position = UDim2.new(0.5, 0, 0.5, 0)
    
    TweenService:Create(self.mainContainer, TweenInfo.new(1.5, Enum.EasingStyle.Back, Enum.EasingDirection.Out), {
        Size = UDim2.new(0, 650, 0, 700),
        Position = UDim2.new(0.5, -325, 0.5, -350)
    }):Play()
    
    -- åˆ›å»ºèƒŒæ™¯åŠ¨ç”»
    self.backgroundGradient.Enabled = true
    local gradientRotation = 0
    RunService.Heartbeat:Connect(function(delta)
        gradientRotation = (gradientRotation + delta * 10) % 360
        self.backgroundGradient.Rotation = gradientRotation
    end)
end

