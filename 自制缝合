local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local UserInputService = game:GetService("UserInputService")
local RunService = game:GetService("RunService")
local Lighting = game:GetService("Lighting")

-- åˆ›å»ºé«˜çº§UIç³»ç»Ÿ
local AdvancedUI = {}
AdvancedUI.__index = AdvancedUI

function AdvancedUI.new()
    local self = setmetatable({}, AdvancedUI)
    self:Initialize()
    return self
end

function AdvancedUI:Initialize()
    self.player = Players.LocalPlayer
    self.gui = Instance.new("ScreenGui")
    self.gui.Name = "PremiumScriptHub"
    self.gui.ResetOnSpawn = false
    self.gui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
    self.gui.Parent = self.player:WaitForChild("PlayerGui")
    
    self:CreateMainContainer()
    self:CreateBackgroundEffects()
    self:CreateHeader()
    self:CreateNavigation()
    self:CreateContentAreas()
    self:CreateScriptList()
    self:CreateAnimations()
    self:CreateParticleSystem()
    self:CreateSoundEffects()
    self:CreateSettingsPanel()
    self:CreateStatisticsPanel()
    self:CreateNotifications()
end

function AdvancedUI:CreateMainContainer()
    -- ä¸»å®¹å™¨
    self.mainContainer = Instance.new("Frame")
    self.mainContainer.Size = UDim2.new(0, 650, 0, 700)
    self.mainContainer.Position = UDim2.new(0.5, -325, 0.5, -350)
    self.mainContainer.AnchorPoint = Vector2.new(0.5, 0.5)
    self.mainContainer.BackgroundColor3 = Color3.fromRGB(15, 15, 25)
    self.mainContainer.BackgroundTransparency = 0.05
    self.mainContainer.BorderSizePixel = 0
    self.mainContainer.ClipsDescendants = true
    self.mainContainer.Parent = self.gui
    
    -- é«˜çº§åœ†è§’
    local corner = Instance.new("UICorner")
    corner.CornerRadius = UDim.new(0, 20)
    corner.Parent = self.mainContainer
    
    -- å¤šå±‚è¾¹æ¡†æ•ˆæœ
    for i = 1, 3 do
        local stroke = Instance.new("UIStroke")
        stroke.Color = Color3.fromRGB(40 + i * 20, 60 + i * 15, 120 + i * 10)
        stroke.Thickness = 2 + i
        stroke.Transparency = 0.3 - (i * 0.1)
        stroke.Parent = self.mainContainer
    end
    
    -- åŠ¨æ€æ¸å˜èƒŒæ™¯
    self.backgroundGradient = Instance.new("UIGradient")
    self.backgroundGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(20, 25, 45)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(30, 35, 65)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(15, 20, 40))
    })
    self.backgroundGradient.Rotation = 45
    self.backgroundGradient.Enabled = false
    self.backgroundGradient.Parent = self.mainContainer
    
    -- èƒŒæ™¯çº¹ç†
    local texture = Instance.new("ImageLabel")
    texture.Size = UDim2.new(1, 0, 1, 0)
    texture.BackgroundTransparency = 1
    texture.Image = "rbxassetid://7743878454"
    texture.ImageTransparency = 0.9
    texture.ScaleType = Enum.ScaleType.Tile
    texture.TileSize = UDim2.new(0, 100, 0, 100)
    texture.Parent = self.mainContainer
end

function AdvancedUI:CreateBackgroundEffects()
    -- åˆ›å»ºèƒŒæ™¯æ¨¡ç³Š
    self.blur = Instance.new("BlurEffect")
    self.blur.Size = 8
    self.blur.Parent = Lighting
    
    -- åˆ›å»ºé¢œè‰²æ ¡æ­£
    self.colorCorrection = Instance.new("ColorCorrectionEffect")
    self.colorCorrection.Brightness = 0.05
    self.colorCorrection.Contrast = 0.1
    self.colorCorrection.Parent = Lighting
end

function AdvancedUI:CreateHeader()
    -- æ ‡é¢˜æ 
    self.header = Instance.new("Frame")
    self.header.Size = UDim2.new(1, 0, 0, 80)
    self.header.Position = UDim2.new(0, 0, 0, 0)
    self.header.BackgroundColor3 = Color3.fromRGB(25, 30, 50)
    self.header.BackgroundTransparency = 0.2
    self.header.BorderSizePixel = 0
    self.header.Parent = self.mainContainer
    
    local headerCorner = Instance.new("UICorner")
    headerCorner.CornerRadius = UDim.new(0, 20)
    headerCorner.Parent = self.header
    
    -- æ ‡é¢˜æ–‡å­—
    self.title = Instance.new("TextLabel")
    self.title.Size = UDim2.new(0, 200, 0, 40)
    self.title.Position = UDim2.new(0.5, -100, 0.5, -20)
    self.title.BackgroundTransparency = 1
    self.title.Text = "âœ¨ é«˜çº§è„šæœ¬ä¸­å¿ƒ âœ¨"
    self.title.TextColor3 = Color3.fromRGB(255, 255, 255)
    self.title.TextSize = 28
    self.title.Font = Enum.Font.GothamBlack
    self.title.TextStrokeTransparency = 0.8
    self.title.TextStrokeColor3 = Color3.fromRGB(100, 150, 255)
    self.title.Parent = self.header
    
    -- å‰¯æ ‡é¢˜
    self.subtitle = Instance.new("TextLabel")
    self.subtitle.Size = UDim2.new(1, 0, 0, 20)
    self.subtitle.Position = UDim2.new(0, 0, 0, 50)
    self.subtitle.BackgroundTransparency = 1
    self.subtitle.Text = "é€‰æ‹©æ‚¨è¦æ‰§è¡Œçš„å¼ºå¤§è„šæœ¬"
    self.subtitle.TextColor3 = Color3.fromRGB(200, 200, 255)
    self.subtitle.TextSize = 14
    self.subtitle.Font = Enum.Font.Gotham
    self.subtitle.Parent = self.header
    
    -- è£…é¥°çº¿æ¡
    local line = Instance.new("Frame")
    line.Size = UDim2.new(1, -40, 0, 2)
    line.Position = UDim2.new(0, 20, 1, -2)
    line.BackgroundColor3 = Color3.fromRGB(100, 150, 255)
    line.BorderSizePixel = 0
    line.Parent = self.header
    
    local lineGradient = Instance.new("UIGradient")
    lineGradient.Color = ColorSequence.new({
        ColorSequenceKeypoint.new(0, Color3.fromRGB(100, 150, 255)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(150, 200, 255)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(100, 150, 255))
    })
    lineGradient.Parent = line
end

function AdvancedUI:CreateNavigation()
    -- å¯¼èˆªæ 
    self.navBar = Instance.new("Frame")
    self.navBar.Size = UDim2.new(1, -40, 0, 50)
    self.navBar.Position = UDim2.new(0, 20, 0, 90)
    self.navBar.BackgroundColor3 = Color3.fromRGB(30, 35, 60)
    self.navBar.BackgroundTransparency = 0.3
    self.navBar.BorderSizePixel = 0
    self.navBar.Parent = self.mainContainer
    
    local navCorner = Instance.new("UICorner")
    navCorner.CornerRadius = UDim.new(0, 12)
    navCorner.Parent = self.navBar
    
    -- å¯¼èˆªæŒ‰é’®
    local navButtons = {
        {Name = "ğŸ  ä¸»é¡µ", Icon = "ğŸ“Š"},
        {Name = "âš¡ è„šæœ¬", Icon = "ğŸ”§"},
        {Name = "âš™ï¸ è®¾ç½®", Icon = "ğŸ“ˆ"},
        {Name = "â„¹ï¸ ä¿¡æ¯", Icon = "ğŸŒŸ"}
    }
    
    for i, buttonData in ipairs(navButtons) do
        local button = Instance.new("TextButton")
        button.Size = UDim2.new(0.25, -5, 1, -10)
        button.Position = UDim2.new((i-1) * 0.25, 5, 0, 5)
        button.BackgroundColor3 = Color3.fromRGB(40, 45, 80)
        button.Text = buttonData.Icon .. " " .. buttonData.Name
        button.TextColor3 = Color3.fromRGB(255, 255, 255)
        button.TextSize = 14
        button.Font = Enum.Font.GothamBold
        button.Parent = self.navBar
        
        local buttonCorner = Instance.new("UICorner")
        buttonCorner.CornerRadius = UDim.new(0, 8)
        buttonCorner.Parent = button
        
        -- æŒ‰é’®äº¤äº’æ•ˆæœ
        button.MouseEnter:Connect(function()
            TweenService:Create(button, TweenInfo.new(0.3), {
                BackgroundColor3 = Color3.fromRGB(60, 65, 120),
                TextColor3 = Color3.fromRGB(200, 230, 255)
            }):Play()
        end)
        
        button.MouseLeave:Connect(function()
            TweenService:Create(button, TweenInfo.new(0.3), {
                BackgroundColor3 = Color3.fromRGB(40, 45, 80),
                TextColor3 = Color3.fromRGB(255, 255, 255)
            }):Play()
        end)
    end
end

function AdvancedUI:CreateContentAreas()
    -- å†…å®¹åŒºåŸŸ
    self.contentArea = Instance.new("Frame")
    self.contentArea.Size = UDim2.new(1, -40, 1, -160)
    self.contentArea.Position = UDim2.new(0, 20, 0, 150)
    self.contentArea.BackgroundTransparency = 1
    self.contentArea.Parent = self.mainContainer
    
    -- è„šæœ¬åˆ—è¡¨å®¹å™¨
    self.scriptListContainer = Instance.new("ScrollingFrame")
    self.scriptListContainer.Size = UDim2.new(1, 0, 1, 0)
    self.scriptListContainer.BackgroundTransparency = 1
    self.scriptListContainer.BorderSizePixel = 0
    self.scriptListContainer.ScrollBarThickness = 8
    self.scriptListContainer.ScrollBarImageColor3 = Color3.fromRGB(100, 150, 255)
    self.scriptListContainer.Parent = self.contentArea
    
    local listLayout = Instance.new("UIListLayout")
    listLayout.Padding = UDim.new(0, 15)
    listLayout.Parent = self.scriptListContainer
end

